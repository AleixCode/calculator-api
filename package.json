{
  "name": "rest-calculator",
  "version": "1.0.0",
  "description": "A Calculator that uses a HATEOAS REST API",
  "main": "index.js",
  "scripts": {
    "start": "node dst/index.js",
    "compile": "./node_modules/.bin/tsc",
    "test": "jest",
    "dev-server": "./node_modules/.bin/nodemon dst/index.js",
    "dev-compile": "./node_modules/.bin/tsc --watch"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "author": "Josh Wulf <josh@magikcraft.io>",
  "license": "ISC",
  "dependencies": {
    "express": "^4.16.2",
    "route-cache": "^0.4.3"
  },
  "devDependencies": {
    "@types/express": "^4.0.39",
    "@types/jest": "^21.1.8",
    "@types/node": "^8.0.58",
    "jest": "^21.2.1",
    "jest-cli": "^21.2.1",
    "nodemon": "^1.12.5",
    "ts-jest": "^21.2.4",
    "tslint": "^5.8.0",
    "typescript": "^2.6.2"
  }
}
